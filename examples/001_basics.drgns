// This is a comment!
/* This is also a comment /* which can be nested */ */

// Statements are newline separated
foo := 1
bar := 2

// They can be inlined with a semicolon
baz := 3; qux := 4

// Declare and use a module
barfoo := "Hi!"
mod my_module {
    foobar := super::barfoo
}
use my_module
my_module::foobar.print()
